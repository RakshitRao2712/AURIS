{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://auris-production.up.railway.app",
    "NEXT_PUBLIC_BACKEND_URL": "https://auris-production.up.railway.app"
  }
}
